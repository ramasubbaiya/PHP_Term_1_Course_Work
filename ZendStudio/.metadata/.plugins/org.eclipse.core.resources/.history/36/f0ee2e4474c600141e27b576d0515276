<html><head></head>
<body>
<?php
session_start();
if(isset($_SESSION['access'])){
    $_SESSION['access'][]=date();
}else{
      $_SESSION['access']=array(date());
      
  }
  foreach($_SESSION['access'] as $v){
        echo $v.'<br>';
  }
  echo count($_SESSION['access']);
  ?>
  <a href='clear_access.php'>Clear Acess</a>
</body>
